	<elmah>
		<security allowRemoteAccess="1" />
		<errorFilter>
			<!-- Reference: http://code.google.com/p/elmah/wiki/ErrorFiltering -->
			<test>
				<and>
					<equal binding="HttpStatusCode" value="404" type="Int32" />
					<regex binding="FilterSourceType.Name" pattern="mail" />
				</and>
			</test>
		</errorFilter>
